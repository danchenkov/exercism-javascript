{
    "root":true,
    "extends":"@exercism/eslint-config-javascript",
    "env":{
        "es2021":true,
        "jest":true
    },
    "parserOptions":{
        "ecmaVersion":12,
        "sourceType":"module",
        "requireConfigFile":"false",
        "babelOptions":{
            "configFile":"./.babelrc"
        }
    },
    "plugins":[
        "@typescript-eslint"
    ],
    "overrides":[
        {
            "files":[
                ".meta/proof.ci.js",
                ".meta/exemplar.js",
                "*.spec.js"
            ],
            "excludedFiles":[
                "custom.spec.js"
            ],
            "extends":"@exercism/eslint-config-javascript/maintainers"
        }
    ]
}